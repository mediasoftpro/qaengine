@model Jugnoon.qa.Models.qaNavViewModel
@inject IStringLocalizer<GeneralResource> GeneralResource
<aside class="sidebar">
    @{
   
        var tModel = new TagListModelView()
        {
            HeadingTitle =  GeneralResource["_labels"].Value,
            Type = (int)TagsBLL.Types.QA,
            Tag_Level = (int)TagsBLL.TagLevel.All,
            Tag_Type = (int)TagsBLL.TagType.Normal,
            TotalRecords = 15,
            Path = "qa/"
        };
        var cModel = new CategoryListModelView()
        {
            Type = (int)CategoryBLL.Types.qa,
            HeadingTitle = GeneralResource["_categories"].Value,
            Path = "qa/"
        };
        var aModel = new ArchiveListModelView()
        {
            Type = ContentTypes.QA,
            Path = "qa/"
        };

        var CategoryViewPath = "~/Views/shared/_Categories_Style_02.cshtml";
        var TagViewPath = "~/Views/shared/_Tags.cshtml";
        var ArchiveViewPath = "~/Views/qa/partials/_Archives.cshtml";
    }


    @await Html.PartialAsync(CategoryViewPath, cModel)
    @await Html.PartialAsync(TagViewPath, tModel)
    @await Html.PartialAsync(ArchiveViewPath, aModel)

    @await Html.PartialAsync("~/Views/partials/ads/ad_336.cshtml", new AdsModelView() { isAdult = false })
</aside>